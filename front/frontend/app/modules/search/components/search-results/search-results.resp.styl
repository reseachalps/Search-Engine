
@media (max-width: 959px) {
  search-results {
    .layout-padding, .layout-padding > *{
      padding: 0 !important
    }
  }
}
@media (max-width: 700px) {
  search-results {
    md-tabs{
      md-pagination-wrapper{
        left: 0 !important
      }
    }
    .results-container{
      p.results-count{
        position: initial;
        float: none;
        padding: 10px 15px;
      }
    }
  }
}